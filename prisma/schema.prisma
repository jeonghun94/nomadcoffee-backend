generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  userName String @unique
  email String @unique
  name String
  location String
  password String 
  avatarURL String?
  githubUserName String?
  createAt DateTime @default(now())
  updateAt DateTime @updatedAt
}